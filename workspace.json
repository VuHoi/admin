{
  "version": 2,
  "projects": {
    "super-admin": {
      "root": "packages/super-admin",
      "sourceRoot": "packages/super-admin/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nx/next:build",
          "outputs": ["packages/super-admin/.next"],
          "defaultConfiguration": "production",
          "options": {
            "outputPath": "packages/super-admin/.next"
          },
          "configurations": {
            "development": {
              "outputPath": "packages/super-admin/.next"
            },
            "production": {
              "outputPath": "packages/super-admin/.next"
            }
          }
        },
        "serve": {
          "executor": "@nx/next:server",
          "defaultConfiguration": "development",
          "options": {
            "buildTarget": "super-admin:build",
            "dev": true
          },
          "configurations": {
            "development": {
              "buildTarget": "super-admin:build:development",
              "dev": true
            },
            "production": {
              "buildTarget": "super-admin:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nx/next:export",
          "options": {
            "buildTarget": "super-admin:build:production"
          }
        },
        "test": {
          "executor": "@nx/jest:jest",
          "outputs": ["{workspaceRoot}/coverage/super-admin"],
          "options": {
            "jestConfig": "jest.config.ts",
            "passWithNoTests": true
          },
          "configurations": {
            "ci": {
              "ci": true,
              "codeCoverage": true
            }
          }
        },
        "lint": {
          "executor": "@nx/eslint:lint",
          "outputs": ["{options.outputFile}"]
        }
      },
      "tags": ["super-admin", "frontend", "application"]
    },
    "vendor": {
      "root": "packages/vendor",
      "sourceRoot": "packages/vendor/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nx/next:build",
          "outputs": ["packages/vendor/.next"],
          "options": {
            "outputPath": "packages/vendor/.next"
          },
          "configurations": {
            "development": {
              "outputPath": "packages/vendor/.next"
            },
            "production": {
              "outputPath": "packages/vendor/.next"
            }
          }
        },
        "serve": {
          "executor": "@nx/next:server",
          "defaultConfiguration": "development",
          "options": {
            "buildTarget": "vendor:build",
            "dev": true
          }
        },
        "test": {
          "executor": "@nx/jest:jest",
          "outputs": ["{workspaceRoot}/coverage/packages/vendor"],
          "options": {
            "jestConfig": "packages/vendor/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nx/eslint:lint",
          "outputs": ["{options.outputFile}"]
        }
      },
      "tags": ["vendor", "library"]
    },
    "system-design": {
      "root": "packages/system-design",
      "sourceRoot": "packages/system-design/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nx/js:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/system-design",
            "main": "packages/system-design/src/index.ts",
            "tsConfig": "packages/system-design/tsconfig.json",
            "assets": ["packages/system-design/*.md"]
          }
        },
        "serve": {
          "executor": "@nx/next:server",
          "defaultConfiguration": "development",
          "options": {
            "buildTarget": "system-design:build",
            "dev": true
          }
        }
      },
      "tags": ["system-design", "library"]
    }
  }
} 